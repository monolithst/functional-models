{
  "branches": 90,
  "lines": 98,
  "functions": 98,
  "statements": 98,
  "extends": "@istanbuljs/nyc-config-typescript",
  "check-coverage": true,
  "all": true,
  "include": ["src/**/!(*.test.*).[tj]s?(x)"],
  "exclude": [
    "src/index.ts",
    "src/**/index.ts",
    "src/_tests_/**/*.*",
    "node_modules",
    ".nyc_output",
    "coverage",
    ".git",
    ".github",
    "features"
  ],
  "reporter": ["html", "lcov", "text", "text-summary"],
  "report-dir": "coverage"
}
