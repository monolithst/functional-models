{
  "name": "functional-models",
  "version": "1.0.13",
  "description": "A library for creating JavaScript function based models.",
  "main": "index.js",
  "scripts": {
    "test": "nyc --all mocha --recursive ./test/**/*.test.js",
    "feature-tests": "./node_modules/@cucumber/cucumber/bin/cucumber-js",
    "coverage": "nyc --all --reporter=lcov npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monolithst/functional-models.git"
  },
  "keywords": [
    "orm",
    "modeling",
    "functional"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "Mike Cornwell",
  "license": "GPLV3",
  "bugs": {
    "url": "https://github.com/monolithst/functional-models/issues"
  },
  "homepage": "https://github.com/monolithst/functional-models#readme",
  "nyc": {
    "all": true,
    "exclude": [
      "coverage/",
      "features/stepDefinitions/*",
      "test/*"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.0",
    "cucumber": "^7.0.0-rc.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-functional": "^3.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^11.1.2"
  },
  "dependencies": {
    "get-random-values": "^1.2.2",
