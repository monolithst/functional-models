{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es2021", "dom", "es2017"],
    "module": "commonjs",
    "declaration": true,
     "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true                                 /* Skip type checking all .d.ts files. */
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
